			if(dct_cnt2==1)begin
				write3<=1;
				indata3<=dct_tmp3;
				addr3<=dct_index_in;
				dct_tmp1<=0;
				dct_tmp2<=0;
				dct_tmp3<=0;
				addr_mfb<=0;
				dct_calc_index<=0;
				dct_index_in<=dct_index_in+1;
				dct_cnt1<=0;
				dct_cnt2<=0;				
			end else begin
				dct_cnt2<=1;
				//dct_tmp3<=dct_tmp1[35:14];
				dct_tmp3<=$signed(dct_tmp2[35:14]);
			end
		end else begin					//DCT1̌vZ
			
			case(dct_cnt1)
				0:begin
					addr_mfb<=0;
					dct_cnt1<=dct_cnt1+1;
				end
				1:begin
					addr_mfb<=1;
					indataA<=outdata_mfb;
					dct_calc_index<=1;
					dct_cnt1<=dct_cnt1+1;
				end
				2:begin
					//f[^
					addr_mfb<=2;
					indataA<=outdata_mfb;
					dct_calc_index<=dct_calc_index+1;
					//o
					dct_tmp2<=outdataX;
					dct_cnt1<=dct_cnt1+1;
				end
				3:begin
					//f[^
					indataA<=outdata_mfb;
					addr_mfb<=addr_mfb+1;
					dct_calc_index<=dct_calc_index+1;
					//o
					//dct_tmp1<=outdataX;				
					dct_tmp2<=dct_tmp2+outdataX;
				end
			endcase
			
		end
	end //dct_index_in
